
public class Caneta {

	String modelo="Bic";
	String cor="Azul";
	double tamanhoPonta=1.3;
	int qntdCarga=50;
	boolean tampada=true;
	public static void main(String[] args) {
		public void rabiscar() {
			if(tampada==true) {
				System.out.println("NAO PODE RABISCAR");
			}else {
				System.out.println("Pode rabiscar");
			}
			
		}
		public void tampar() {
			tampada=true;
			System.out.println("Foi tampada:"+tampada);
		}
	}

}
