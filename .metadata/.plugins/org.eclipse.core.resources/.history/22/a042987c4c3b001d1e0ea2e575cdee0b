
public class Caneta {
	public  String modelo;
	private  float ponta;
	
	
	public String getModelo() {
		return modelo;
	}
	public  void setModelo(String m) {
		this.modelo = m;
	}
	public float getPonta() {
		return ponta;
	}
	public void setPonta(float p) {
		this.ponta = p;
	}
	public void status() {
		System.out.println("Modelo:"+modelo);
		System.out.println("Ponta:"+ponta);
	}
	
	
}
