
public class Caneta {

	String modelo;
	String cor;
	double tamanhoPonta;
	int qntdCarga;
	boolean tampada;
	void status() {
		System.out.println("Uma caneta"+ this.cor);
		System.out.println("esta tampada :" +this.tampada);
	}
	void rabiscar() {
			if(tampada==true) {
				System.out.println("NAO PODE RABISCAR");
			}else {
				System.out.println("Pode rabiscar");
			}
			
		}
	void tampa() {
			tampada=true;
			System.out.println("Foi tampada:"+tampada);
		}
	void destampar() {
		
	}
	

}
